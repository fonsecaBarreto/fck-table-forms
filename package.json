    "@types/react-dom": "^17.0.11",
    "react": ">=17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "axios": "^0.26.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": "^17.0.2",
